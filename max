#This program will return the maximum value in a list
def max(l):
    if len(l) == 0:
        return "NA"
    elif len(l) == 1:
        return l[0]
    else:
        if l[0]>l[1]:
            l.pop(1)
            return max(l)
        else:
            l.pop(0)
            return max(l)
max([2, 6, 4, 8, 7])
